{
  "singleQuote": true,
  "arrowParens": "always",
  "max-len": [
    "error",
    100,
    2,
    {
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignoreRegExpLiterals": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true
    }
  ],
  "tabWidth": 2,
  "useTabs": false
}